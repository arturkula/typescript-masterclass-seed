{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es6"],
    "outDir": "dist",
    "noEmitOnError": true,
    "strict": true,
    "traceResolution": true,
    "diagnostics": true,
    // "listFiles": true,
    "listEmittedFiles": true,
    "pretty": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true
  }
}
